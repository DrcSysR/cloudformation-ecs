{
    "name": "$CONTAINER_NAME",
    "image": "$IMAGE",
    "essential": true,
    "memoryReservation": 512,
    "portMappings": [
        {
            "hostPort": 3000,
            "containerPort": 3000
        }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "$ENV_NAME-LG",
            "awslogs-region": "$AWS_DEFAULT_REGION",
            "awslogs-stream-prefix": "$CONTAINER_NAME"
        }
    }
}
